    <script>
      window.onload = function() {
      
      var chart = new CanvasJS.Chart("chartContainer", {
          theme: "light2", // "light1", "light2", "dark1", "dark2"
          exportEnabled: true,
          animationEnabled: true,
          title: {
              text: "Previous Election Record"
          },
          data: [{
              type: "pie",
              startAngle: 25,
              // toolTipContent: "<b>{label}</b>: {y}%",
              // showInLegend: "true",
              // legendText: "{label}",
              indexLabelFontSize: 16,
              indexLabel: "{label} - {y}%",
              dataPoints: [
                  { y: 51.08, label: "Party 1" },
                  { y: 27.34, label: "Party 2" },
                  { y: 10.62, label: "Party 3" },
                  { y: 5.02, label: "party 4" },
                  { y: 4.07, label: "party 5" },
                  { y: 1.22, label: "party 6" },
                 
              ]
          }]
      });
      chart.render();
      
      }
